package pages;

import org.openqa.selenium.WebDriver;

import dsl.DSL;

public class CadastroPage {

	
	private DSL dsl;
	
	public CadastroPage(WebDriver driver) {
		dsl = new DSL(driver);
	}
	
	
	public void setUserNameByName(String nome) {
		dsl.escreveByName("usernameRegisterPage", nome);
	}
	
	
}
